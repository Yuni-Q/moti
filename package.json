{
	"name": "next-with-typescript",
	"version": "1.0.0",
	"main": "server.js",
	"scripts": {
		"dev": "nodemon server.local.ts",
		"build": "ANALYZE=false NODE_ENV=production next build",
		"start": "ANALYZE=false NODE_ENV=production ts-node ./server.local.ts",
		"predeloy": "git pull && npm install && npm run build",
		"deploy": "NODE_ENV=production pm2 start ts-node -- -P tsconfig.server.json ./server.local.ts",
		"export": "npm run build && next export",
		"type-check": "tsc",
		"lint": "concurrently \"npm run lint:ts\" \"npm run lint:style\"",
		"lint:es": "eslint --ext .ts --ext .tsx . --fix --quiet",
		"lint:style": "stylelint \"./**/*.{scss,css}\" --fix",
		"prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx}'",
		"make:token": "web-push generate-vapid-keys",
		"watch": "jest --watchAll"
	},
	"dependencies": {
		"@types/styled-components": "^5.1.4",
		"@types/web-push": "^3.3.0",
		"aws-serverless-express": "^3.3.8",
		"axios": "^0.19.2",
		"cookie": "^0.4.1",
		"cookie-parser": "^1.4.5",
		"cross-env": "^7.0.2",
		"dayjs": "^1.9.5",
		"dotenv": "^8.2.0",
		"dotenv-webpack": "^2.0.0",
		"esm": "^3.2.25",
		"express": "^4.17.1",
		"express-http-proxy": "^1.6.2",
		"googleapis": "^59.0.0",
		"lodash": "^4.17.20",
		"morgan": "^1.9.1",
		"next": "^10.0.1",
		"nuka-carousel": "^4.7.2",
		"pm2": "^4.5.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-helmet": "^6.1.0",
		"react-redux": "^7.2.2",
		"styled-components": "^5.2.1",
		"ts-node": "^9.1.1",
		"universal-cookie": "^4.0.4",
		"url": "^0.11.0",
		"url-loader": "^3.0.0",
		"web-push": "^3.4.4"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "^10.0.1",
		"@testing-library/jest-dom": "^5.11.6",
		"@testing-library/react": "^11.1.2",
		"@types/aws-serverless-express": "^3.3.3",
		"@types/cookie-parser": "^1.4.2",
		"@types/express": "^4.17.8",
		"@types/express-http-proxy": "^1.6.1",
		"@types/jest": "^26.0.15",
		"@types/jest-plugin-context": "^2.9.4",
		"@types/lodash": "^4.14.165",
		"@types/morgan": "^1.9.2",
		"@types/node": "^12.19.3",
		"@types/react": "^16.9.56",
		"@types/react-dom": "^16.9.9",
		"@types/react-helmet": "^6.1.0",
		"@typescript-eslint/eslint-plugin": "^4.6.1",
		"@typescript-eslint/parser": "^4.6.1",
		"babel-eslint": "^10.1.0",
		"babel-plugin-styled-components": "^1.11.1",
		"compression-webpack-plugin": "^6.0.5",
		"eslint": "^7.13.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^4.3.0",
		"jest": "^26.6.3",
		"jest-plugin-context": "^2.9.0",
		"lint-staged": "^10.5.1",
		"nodemon": "^2.0.7",
		"prettier": "^2.1.2",
		"stylelint": "^13.7.2",
		"typescript": "^4.0.5",
		"webpack": "^4.44.2"
	},
	"license": "ISC",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"eslint --fix",
			"git add"
		],
		"*.{css,scss}": [
			"stylelint --fix",
			"git add"
		]
	}
}

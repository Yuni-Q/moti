{
	"name": "next-with-typescript",
	"version": "1.0.0",
	"main": "server.js",
	"scripts": {
		"dev": "nodemon",
		"build": "ANALYZE=false NODE_ENV=production next build",
		"start": "ANALYZE=false NODE_ENV=production node server.js",
		"predeloy": "npm run build",
		"deploy": "git pull && npm install && pm2 start ecosystem.config.js --env production",
		"export": "npm run build && next export",
		"type-check": "tsc",
		"lint": "concurrently \"npm run lint:ts\" \"npm run lint:style\"",
		"lint:es": "eslint --ext .ts --ext .tsx . --fix --quiet",
		"lint:style": "stylelint \"./**/*.{scss,css}\" --fix",
		"prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx}'",
		"make:token": "web-push generate-vapid-keys"
	},
	"dependencies": {
		"@ant-design/icons": "^4.2.2",
		"@types/next-redux-saga": "^3.0.2",
		"@types/react-helmet": "^5.0.14",
		"@types/react-redux": "^7.1.5",
		"@types/redux-actions": "^2.6.1",
		"@types/styled-components": "^5.1.3",
		"@types/web-push": "^3.3.0",
		"antd": "^4.6.3",
		"axios": "^0.19.2",
		"cookie": "^0.4.1",
		"cookie-parser": "^1.4.5",
		"cross-env": "^7.0.2",
		"dayjs": "^1.8.35",
		"dotenv": "^8.2.0",
		"dotenv-webpack": "^2.0.0",
		"express": "^4.17.1",
		"faker": "^4.1.0",
		"googleapis": "^59.0.0",
		"immer": "^7.0.8",
		"isomorphic-unfetch": "3.0.0",
		"lodash": "^4.17.20",
		"morgan": "^1.9.1",
		"next": "^9.5.3",
		"next-redux-wrapper": "^6.0.1",
		"next-routes": "^1.4.2",
		"nuka-carousel": "^4.7.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-helmet": "^5.2.1",
		"react-redux": "^7.2.1",
		"redux": "^4.0.5",
		"redux-actions": "^2.6.5",
		"redux-devtools-extension": "^2.13.8",
		"redux-saga": "^1.1.3",
		"styled-components": "^5.2.0",
		"universal-cookie": "^4.0.3",
		"url": "^0.11.0",
		"url-loader": "^3.0.0",
		"web-push": "^3.4.4"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "^9.5.3",
		"@types/lodash": "^4.14.161",
		"@types/node": "^12.12.56",
		"@types/react": "^16.9.49",
		"@types/react-dom": "^16.9.1",
		"@typescript-eslint/eslint-plugin": "^3.10.1",
		"@typescript-eslint/parser": "^3.10.1",
		"babel-eslint": "^10.1.0",
		"babel-plugin-styled-components": "^1.11.1",
		"compression-webpack-plugin": "^4.0.1",
		"eslint": "^7.8.1",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.20.6",
		"eslint-plugin-react-hooks": "^4.1.0",
		"husky": "^4.3.0",
		"lint-staged": "^10.3.0",
		"nodemon": "^2.0.2",
		"prettier": "^2.1.1",
		"stylelint": "^13.7.0",
		"typescript": "^4.0.2",
		"webpack": "^4.44.1"
	},
	"license": "ISC",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"eslint --fix",
			"git add"
		],
		"*.{css,scss}": [
			"stylelint --fix",
			"git add"
		]
	}
}
